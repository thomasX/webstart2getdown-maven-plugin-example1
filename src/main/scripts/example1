#!/bin/bash

CACHE_DIR="${HOME}/.cache/example1"
mkdir -p "${CACHE_DIR}"
pushd "${CACHE_DIR}"
for i in /usr/lib/example1/* ; do
	basename="$(basename ${i})"
	if [ ! -f "${basename}" -a ! -L "${basename}" ]; then
		ln -s "${i}" . 2>/dev/null
	fi
done
java -jar /usr/lib/example1/getdown.jar . 